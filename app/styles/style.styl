open = 'Open Sans', sans-serif
item-color = #283645
pink-color = #d22856
sprite = "file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/sprite-3@1.svg"

body
article
div
header
footer
main
ul
li
a
h1
h2
h3
h4
  margin 0
  padding 0
  list-style none
  box-sizing border-box

html
  margin 0
  padding 0

body
  width 100%
  margin 0
  padding 0
  
  font-family open
  color item-color
  background-color #fff


@media screen and (min-width:0px) and (max-width:699px)
  body
    min-width 320px
    
    font-size 4.375vw
    line-height 4.375vw
    
  .main-header
    width 100%
    min-height 66px

    color #fff

    background-color item-color
    background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/background-1@1x.jpg")
    background-repeat no-repeat
    background-position center
    background-size 100% 100%
    
  .header-head
    width 100%
    height 20.625vw
    padding 6.875vw 7.19%
    
    background-color rgba(0,0,0,0.4)
    clear both

  .logo
    float left
    width 23.125vw
    height 6.875vw
    
    background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/Pink-logo-1.png")
    background-repeat no-repeat
    background-position center
    background-size 100%
    
    &:hover
      opacity 0.5
      
    &:active
      opacity 0.6
      
  .main-menu__btn-burger
    display block
    width 15.625vw
    height 6.875vw
    float right
    
    svg
      width 15.625vw
      height 6.875vw
      fill #fff
      
  .main-menu__btn-cross
    display none
    width 6.875vw
    height 6.875vw
    float right
    
  .main-menu
    width 100%
    margin 0
    
    position absolute
    top 20.625vw
    left 0
    z-index 5
    
  .main-menu__item
    display none
    width 100%
    padding 5vw 0
    margin 0
    
    font-size 5.625vw
    line-height 10vw
    font-weight 700
    text-align center
    text-transform uppercase
    
    background-color #283645
    cursor pointer
    
    border-bottom 0.31vw solid #000
    border-top 0.31vw solid rgba(#fff, 0.3)
    
    &:hover
      color pink-color
     
    &:active
      color rgba(pink-color, 0.3)
      
  .header-body
    position relative
    height 115.5vw
    margin 0
    margin-bottom 80vw
    
    background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/iphone-hand-1.png")
    background-repeat no-repeat
    background-position left bottom
    background-size 80.625vw 89.0625vw
    
    &:after
      content ""
      display block
      width 100%
      height 77.5vw
      
      position absolute
      top 100%
      
      background-color item-color
      z-index 0

  .header-body__content
    position relative
    
    display block
    width 87.5%
    margin 0 auto
    margin-top 10vw
    
    text-align center
    z-index 2
    
  .header-body__tagline
    display block
    width 100%
    height 18.125vw
    margin 0 auto
    padding-bottom 97.19vw
    
    text-align center
    font-size 7.5vw
    line-height 9.063vw
    font-weight 300
    
    border-bottom 1px solid transparent

  .header-body__btn
    display block
    padding 5.3vw 11.25vw
    margin 0 auto
    margin-top 8.75vw
    margin-bottom 8.44vw
    
    font-size 5.625vw
    line-height 5.625vw
    font-weight 700
    text-align center
    color #fff
    
    background-color transparent
    border 0.94vw solid #fff
    border-radius 7.8vw
    
    &:hover
      background-color #fff
      color pink-color
    
    &:active
      color rgba(#d22856, 0.3)

  .header-body__social-btn 
    width 73.2%
    height 57px
    margin 0 auto
    margin-bottom 7.5vw

  .btn-apple
    display inline-block
    width 46px
    height 57px
    margin-right 17.56%
    
    background-image url(sprite)
    background-repeat no-repeat
    background-position 0 0
    background-size 85px 425px

  .btn-android
    display inline-block
    width 42px
    height 48px
    margin-right 17.56%
    
    background-image url(sprite)
    background-repeat no-repeat
    background-position 0px -59.26px
    background-size 85px 425px

  .btn-windows
    display inline-block
    width 44px
    height 42px
    margin-bottom 3px
    
    background-image url(sprite)
    background-position 0px -112.2px
    background-size 85px 425px
    background-repeat no-repeat

  .header-body__note
    display block
    width 73.44vw
    margin 0 auto
    padding-bottom 8.75vw
    
    font-size 4.4vw
    line-height 7.5vw
    font-weight 300
    
  .main-content 
    width 100%
    
  .info
    width 100%
    padding-top 13.75vw

  .info__headline
    width 70%
    margin 0 auto
    margin-bottom 10.625vw
    
    text-align center
    font-size 7.5vw
    line-height 8.75vw
    font-weight 300
    

  .info__subheadline
    display block
    width 70%
    margin 0 auto
    margin-bottom 9.1vw
    
    text-align center
    font-size 7.5vw
    line-height 8.75vw
    font-weight 300

  .info-columns
    position relative
    clear both
    width 100%
    height 245.31vw
    margin-bottom 0
    padding 0 8.44%
    
    background-color #f2f2f2
    
    font-size 4.375vw
    line-height 7.5vw
    font-weight normal
    text-align center
    
    z-index 3
    
    &:after
      content ""
      display block
      width 0
      height 0
      
      position absolute
      top 0
      left 0
      
      border-top 33px solid #fff
      border-left 48.5vw solid #fff
      border-right 48.5vw solid transparent
      border-bottom 33px solid transparent
      
      z-index 1

    &:before
      content ""
      display block
      width 55.71%
      height 86.875vw
      
      position absolute
      left 50%
      bottom 0
      margin-left -27.855%
      
      background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/iphone-screen-2-3@1x.png")
      background-repeat no-repeat
      background-position center
      background-size contain
      
  .info__column1 
    width 100% 
    margin-bottom 10.31vw
    padding-top 26.56vw
    
    text-align center
    z-index 3

  .info__column2 
    width 100%
    margin-bottom 10.31vw

  .info__column3 
    width 100%
    margin-bottom 98.75vw

  .column__headline
    margin-bottom 10px
    
    font-weight 700
    line-height 7.5vw
    font-size 5.625vw
    color #000

  .info-img1 
    display none
    
  .info-img2 
    display none

  .info-img3 
    display none
    
  .effect
    display none

  .review
    clear both
    width 100%
    padding 14.06vw 6.25% 13.13vw
    
    position relative    
    text-align center
    

  .review__recall
    width 100%
    margin 0 auto
    margin-bottom 13.125vw
    padding-top 13.75vw
    
    line-height 9.375vw
    font-size 4.375vw

  .review__author
    display block
    width 100%
    
    position absolute
    top 13.75vw
    left 0
    
    text-align center
    color #000
    font-weight 700
    font-size 5.625vw
    line-height 9.375vw

  .review__author-info
    display none
  
  .review-prew 
    display none
    
  .review-next 
    display none
    
    
  .review__item-hide
    display none
    
  .review__toggle
    display block
    height 3.75vw    
    
    input
      display none
      
    label
      position relative
      display inline-block
      width 1.875vw
      height 1.875vw
      
      border-radius 50%
      background-color lightgray
      border 0.94vw solid #fff
      
  input[type=radio]#review1:checked~label[for="review1"]
    height 1.875vw
    width 1.875vw

    background-color transparent
    border 0.94vw solid item-color

  input[type=radio]#review2:checked~label[for="review2"]
    height 1.875vw
    width 1.875vw

    background-color transparent
    border 0.94vw solid item-color
    
  input[type=radio]#review3:checked~label[for="review3"]
    height 1.875vw
    width 1.875vw

    background-color transparent
    border 0.94vw solid item-color

  .price
    position relative
    height 139.38vw
    padding 28.44vw 0 14.69vw
    box-sizing border-box
    
    overflow hidden
    background-color #f2f2f2

  .price::after
    content ''
    
    position absolute
    top 0
    left 0
    
    display block
    width 0
    height 0
    
    border 33px solid #fff
    border-left 48.5vw solid #fff
    border-right 48.5vw solid transparent
    border-bottom 33px solid transparent

  .price-table
    height 87.81vw
    width 350vw
    margin-bottom 12.5vw
    
    position absolute
    left -170vw
    
    text-align left
    vertical-align middle
    background-color #fff
    border-collapse collapse
    
  .price-table td 
    border 2px solid #e5e5e5
  
  .price-table__headline 
    color #fff
  
  .price-table__headline th
    text-align center
    text-transform uppercase
    font-weight 700
    font-size 5.625vw
    line-height 9.375vw
    
    background-color #283645
    border 0.625vw solid #e5e5e5
    
  .price-table__headline .invisible-cell 
    background-color #f2f2f2
    border none

  .price-table__headline span
    display block
    
    font-weight 300
    font-size 7.5vw
    line-height 9.375vw
    
  .price-table__headline .hit 
    position relative
    overflow hidden

  .price-table__headline .hit::before
    content 'хит'
    
    display block
    width 32.19vw
    height 9.69vw
    
    position absolute
    left -9vw
    top 2vw
    
    font-size 4.375vw
    line-height 9.375vw
    font-weight 700
    
    background-color #d22856
    transform rotate(-45deg)
    
  .price-table__row td:nth-child(1),
  .price-table__row td:nth-child(2),
  .price-table__row td:nth-child(3),
  .price-table__row td:nth-child(4)
    width 87.5vw
    text-align left
    
    span
      font-size 4.375vw
      line-height 9.375vw
      padding 4.69vw 6.25vw 4.69vw 7.8vw
      text-transform uppercase
      
  .price-table__row .price-table__cell--check 
    text-align center
    position relative
    
    &::after
      content ''
      display block
      width 29px
      height 22px
      margin-top -10px
      
      position absolute
      top 50%
      right 6.25vw
      
      background-image url(sprite)
      background-repeat no-repeat
      background-position 0 -234.35px
      background-size 85px 425px

  .price-table__row .price-table__cell--cross
    text-align center
    position relative
    
    &::after
      content ''
      display block
      width 26px
      height 26px
      margin-top -13px
      
      position absolute
      top 50%
      right 6.25vw
      
      background-image url(sprite)
      background-repeat no-repeat
      background-position 0 -261.25px
      background-size 85px 425px
      
  .price__toggle
    display block
    width 100%
    
    position absolute
    bottom 14.69vw
    
    text-align center
    height 3.75vw
    
    input
      display none
      
    label
      display inline-block
      width 1.875vw
      height 1.875vw
      
      border-radius 50%
      background-color lightgray
      border 0.94vw solid #f2f2f2
      
    input[type=radio]#row1:checked~label[for="row1"]
      height 1.875vw
      width 1.875vw
      
      background-color #f2f2f2
      border 0.94vw solid item-color
    
   input[type=radio]#row2:checked~label[for="row2"]
      height 1.875vw
      width 1.875vw
      
      background-color #f2f2f2
      border 0.94vw solid item-color
    
   input[type=radio]#row3:checked~label[for="row3"]
      height 1.875vw
      width 1.875vw
      
      background-color #f2f2f2
      border 0.94vw solid item-color

  .contacts
    width 100%
    height 130vw
    
    position relative
    padding-top 15vw
    
    text-align center
    font-size 5.625vw
    line-height 5.625vw
    font-weight 300
    
    z-index 3

  .contacts__headline
    width 73.44vw
    margin 0 auto
    margin-bottom 12.19vw
    
    text-align center
    font-size 7.5vw
    line-height 9.06vw
    font-weight 300

  .contacts__tel
    display inline-block
    width 100%
    margin-bottom 8.125vw
    
    vertical-align top

  .contacts__tel span
    display block
    margin-bottom 2.19vw
    font-weight 700

  .contacts__tel a
    display block
    font-size 7.5vw
    line-height 7.5vw
    
    color #d22856
    text-decoration none

  .contacts__address
    display inline-block
    width 100%
    
    position absolute
    bottom 15.5vw
    
    vertical-align top

  .contacts__address span
    display block
    margin-bottom 2.19vw
    font-weight 700

  .contacts__address a
    display block
    
    font-size 7.5vw
    line-height 7.5vw
    color #d22856
    text-decoration none

  .contacts__email
    display inline-block
    width 100%
    margin-bottom 46vw
    
    vertical-align top

  .contacts__email span
    display block
    margin-bottom 2.19vw
    font-weight 700

  .contacts__email a
    display block
    
    font-size 7.5vw
    line-height 7.5vw
    color #d22856
    text-decoration none

  .contacts__map
    height 62.5vw
    position relative
    
    background-color rgba(0,0,0,0.5)
    z-index 1

    iframe
      width 100%
      height 62.5vw

    &:before
      content ''
      display block
      width 0
      height 0
      margin 0 auto
      
      position absolute
      top -1px
      
      border-top 10.31vw solid #fff
      border-left 48.5vw solid transparent
      border-right 48.5vw solid transparent

  .main-footer
    display none
    
  @media (min-resolution: 144dpi ), (min-resolution: 1.5dppx )
    .main-header
      background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/background-1@2x.jpg")
      background-size cover
    
    .header-body
      background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/iphone-hand-1@2x.png")
    
    .info-columns:before
      background-image url("file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/iphone-screen-2-3@2x.png")
      background-repeat no-repeat
      background-position center
      background-size contain
      
