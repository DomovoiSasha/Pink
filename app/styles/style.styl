open = 'Open Sans', sans-serif
item-color = #283645
pink-color = #d22856
sprite = "file:///D:/%D0%9E%D0%B1%D1%83%D1%87%D0%B5%D0%BD%D0%B8%D0%B5/%D0%9F%D1%80%D0%BE%D0%B5%D0%BA%D1%82%D1%8B/Pink/dist/images/sprite-3@1.svg"

body
article
div
header
footer
main
ul
li
a
h1
h2
h3
h4
  margin 0
  padding 0
  list-style none
  box-sizing border-box




body
  margin 0 auto
  font-family open
  color item-color
  background-color #fff

.main-header
  width 100%
  min-height 66px
  background-color item-color
  color #fff
  background-repeat no-repeat
  background-position center
  background-size 100%

.header-head
  background-color rgba(0,0,0,0.4)
  clear both

.logo
  float left
  background-repeat no-repeat
  background-position top
  height  40px
  width 146px
  
  &:hover
      background-position bottom

  &:active
      background-position top
      opacity 0.3
  
.main-menu 
    float right
  
.hader-body
  position relative
  margin 0
      
.header-body__content 
    position relative
    text-align center
    
.header-body__tagline 
    text-align center
    display block
    margin 0 auto
    
 .header-body__btn 
    display block
    text-align center
    background-color transparent
    color #fff
    border 3px solid #fff
    border-radius 30px
    margin 0 auto
        
    &:hover
      background-color #fff
      color pink-color
      
    &:active
      color rgba(#d22856, 0.3)

.header-body__social-btn 
    margin 0 auto
    
.btn-apple,
.btn-android,
.btn-windows
    display inline-block
    background-repeat no-repeat

.header-body__note 
    display block
    
.main-content 
    width 100%
    
.info 
    width 100%
    
.info__headline ,
.info__subheadline
    text-align center
    margin 0 auto
    
.info-columns 
    position relative
    clear both
    width 100%
    background-color #f2f2f2
    
.effect 
    width 100%
    background-color #f2f2f2
    clear both
    
.effect__diagram 
    position relative
    float left
    
    svg
      fill rgb(222, 222, 222)
      
    p
      position absolute
      box-sizing border-box
      text-align center
      vertical-align middle
      color pink-color
      display block
      border-radius 50%
      
.effect__description 
    float right
    
.effect__link 
    color pink-color
    text-decoration underline
    
.review 
    clear both
    width 100%
    position relative
    text-align center
    
.review__recall
  margin 0 auto
  
.review__author 
    color #000
    display block
  
.review__item-hide 
    display none
    
.review-prew,
.review-next
    display inline-block
    position absolute
    border none
    background-color transparent
    cursor pointer
    
    &:before
      content ''
      display block
      position absolute
      
  .price 
    background-color #f2f2f2
    position relative
    
.price-table 
    width 100%
    text-align left
    vertical-align middle
    background-color #fff
    border-collapse collapse
    
.price-table td 
    border 2px solid #e5e5e5 
    
.price-table__headline 
    color #fff
    
.price-table__headline th 
    background-color #283645
    border 2px solid #e5e5e5
    text-align center
    text-transform uppercase
    
.price-table__headline .invisible-cell 
    background-color #f2f2f2
    border none
    
.price-table__headline span 
    display block
    
.price-table__headline .hit 
    position relative
    overflow hidden
    
.price-table__headline .hit::before 
    content 'хит'
    position absolute
    display block
    background-color #d22856
    
.price-table__row .price-table__cell--check 
    text-align center
    position relative
    
    &:after
      content ''
      display block
      position absolute
      background-repeat no-repeat
      
.price-table__row .price-table__cell--cross 
    text-align center
    position relative
    
    &:after
      content ''
      display block
      position absolute
      background-repeat no-repeat
      
.contacts
    width 100%
    text-align center

  .contacts__headline 
    text-align center
    margin 0 auto
    
.contacts__tel,
.contacts__address,
.contacts__email
    display inline-block
    vertical-align top
    
    span
      display block
      
    a
      color #d22856
      display block
      text-decoration none
    
.contacts__map
    position relative
    background-color rgba(0,0,0,0.5)

    iframe
      width 100%
      
.logo--footer 
    display inline-block
    background-position bottom
    background-repeat no-repeat
    background-size 100%
    
    &:hover
    &:active
      opacity 0.5
    
.social-btn 
    display inline-block
      
.social-btn .social-btn__twitter,
.social-btn .social-btn__facebook,
.social-btn .social-btn__youtube
    display inline-block
    position relative
    color transparent
    overflow hidden
    border-radius 50%
        
    &:hover
      border-color item-color
      
    &:active
      border-color #f2f2f2
      opacity 0.3
    
    &:after
      content ''
      display block
      position absolute
      background-repeat no-repeat
      
.copywright 
    display inline-block
    float right
    
.copywright__creator,
.copywright__customer
    display inline-block
    color transparent
    overflow hidden
    background-repeat no-repeat
    

    
    
